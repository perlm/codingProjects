<script>
	var queryString = '';
	var queryKey = jQuery.parseUri(window.location.href).queryKey;
	for(var key in queryKey){
		if(queryKey.hasOwnProperty(key)){
			queryString+=' '+key+' '+queryKey[key];
		}
	}

	jQuery('#searchForm .txt-search').val(decodeURIComponent(window.location.pathname.replace(/\//g, ' ') + queryString));

	jQuery('#urllink').html(window.location.href)
</script>
